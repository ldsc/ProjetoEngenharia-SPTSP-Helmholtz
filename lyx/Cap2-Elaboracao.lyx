\begin_layout Standard
Com estas equações é possível obter diversas propriedades termodinâmicas,
 a partir das relações a seguir:
\end_layout

\begin_layout Standard
Pressão
\begin_inset Formula $\left[Bar\right]$
\end_inset

:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
\frac{p(\delta,\tau)}{\rho RT}=1+\delta\phi_{\delta}^{r}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
Entropia
\begin_inset Formula $\left[\frac{kJ}{kg\ldotp K}\right]$
\end_inset

:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
\frac{s(\delta,\tau)}{R}=\tau(\phi_{\tau}^{o}+\phi_{\tau}^{rM=})-\phi^{o}-\phi^{r}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
Energia Interna
\begin_inset Formula $\left[\frac{kJ}{kg}\right]$
\end_inset

:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
\frac{u(\delta,\tau)}{RT}=\tau(\phi_{\tau}^{o}+\phi_{\tau}^{r})
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
Entalpia
\begin_inset Formula $\left[\frac{kJ}{kg}\right]$
\end_inset

:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
\frac{h(\delta,\tau)}{RT}=1+\tau(\phi_{\tau}^{o}+\phi_{\tau}^{r})+\delta\phi_{\delta}^{r}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
Energia livre de Gibbs
\begin_inset Formula $\left[\frac{kJ}{kg}\right]$
\end_inset

:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
\frac{g(\delta,\tau)}{RT}=1+\phi^{o}+\phi^{r}+\delta\phi_{\delta}^{r}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
Capacidade calorífica isocórica 
\begin_inset Formula $\left[\frac{kJ}{kg\ldotp K}\right]$
\end_inset

 :
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
\frac{c_{v}(\delta,\tau)}{R}=-\tau^{2}(\phi_{\tau\tau}^{o}+\phi_{\tau\tau}^{r})
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
Capacidade calorífica isobárica 
\begin_inset Formula $\left[\frac{kJ}{kg\ldotp K}\right]$
\end_inset

 :
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
\frac{c_{p}(\delta,\tau)}{R}=-\tau^{2}(\phi_{\tau\tau}^{o}+\phi_{\tau\tau}^{r})+\frac{(1+\delta\phi_{\delta}^{r}-\delta\tau\phi_{\delta\tau}^{r})^{2}}{1+2\delta\phi_{\delta}^{r}+\delta^{2}\phi_{\delta\delta}^{r}}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
Velocidade do som
\begin_inset Formula $\left[\frac{m}{s}\right]$
\end_inset


\begin_inset Formula $:$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
\frac{w^{2}(\delta,\tau)}{RT}=1+2\delta\phi_{\delta}^{r}+\delta^{2}\phi_{\delta\delta}^{r}-\frac{(1+\delta\phi_{\delta}^{r}-\delta\tau\phi_{\delta\tau}^{r})^{2}}{\tau^{2}(\phi_{\tau\tau}^{o}+\phi_{\tau\tau}^{r})}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
Coeficiente de Joule-Thomson
\begin_inset Formula $\left[\frac{K}{MPa}\right]$
\end_inset

:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
\mu R\rho=\frac{-(\delta\phi_{\delta}^{r}+\delta^{2}\phi_{\delta\delta}^{r}+\delta\tau\phi_{\delta\tau}^{r})}{(1+\delta\phi_{\delta}^{r}-\delta\tau\phi_{\delta\tau}^{r})^{2}-\tau^{2}(\phi_{\tau\tau}^{o}+\phi_{\tau\tau}^{r})(1+2\delta\phi_{\delta}^{r}+\delta^{2}\phi_{\delta\delta}^{r})}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
Coeficiente de enforcamento isotérmico
\begin_inset Formula $\left[\frac{kJ}{kg\cdotp MPa}\right]$
\end_inset

:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
\delta_{T}\rho=\frac{1+\delta\phi_{\delta}^{r}-\delta\tau\phi_{\delta\tau}^{r}}{1+2\delta\phi_{\delta}^{r}+\delta^{2}\phi_{\delta\delta}^{r}}
\end{equation}

\end_inset


\end_layout

\begin_layout Standard
Coeficiente de temperatura-pressão isentrópico
\begin_inset Formula $\left[\frac{K}{MPa}\right]$
\end_inset

:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{equation}
\beta_{s}\rho R=\frac{1+\delta\phi_{\delta}^{r}-\delta\tau\phi_{\delta\tau}^{r}}{(1+\delta\phi_{\delta}^{r}-\delta\tau\phi_{\delta\tau}^{r})^{2}-\tau^{2}(\phi_{\tau\tau}^{o}+\phi_{\tau\tau}^{r})(1+2\delta\phi_{\delta}^{r}+\delta^{2}\phi_{\delta\delta}^{r})}
\end{equation}

\end_inset


\end_layout

